<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hfad.stonehillhousinghelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StonehillHousingHelper">
        <activity
            android:name=".temp"
            android:label="@string/title_activity_temp"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar"></activity>
        <activity
            android:name=".ViewHouseActivity"
            android:label="@string/title_activity_view_house"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".HousingSearchActivity"
            android:label="@string/title_activity_housing_search"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".ViewProfileActivity"
            android:label="@string/title_activity_view_profile"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".NewPostActivity"
            android:label="@string/title_activity_new_post"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".PostActivity"
            android:label="@string/title_activity_post"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".NewUserActivity"
            android:label="@string/title_activity_new_user"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.StonehillHousingHelper.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>